<h1>Listing urls</h1>

<table>
  <tr>
    <th>Url in</th>
    <th>Url out</th>
    <th>Http status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @urls.each do |url| %>
  <tr>
    <td><%= url.url_in %></td>
    <td><%= url.url_out %></td>
    <td><%= url.http_status %></td>
    <td><%= link_to 'Show', url %></td>
    <td><%= link_to 'Edit', edit_url_path(url) %></td>
    <td><%= link_to 'Destroy', url, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Url', new_url_path %>
